<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="org.eclipse.sed.ifl.commands.category"
            name="Fault Localization">
      </category>
      <command
            categoryId="org.eclipse.sed.ifl.commands.category"
            name="Start iFL session"
            id="org.eclipse.sed.ifl.commands.startCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.eclipse.sed.ifl.commands.startCommand"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="org.eclipse.sed.ifl.menus.mainMenu"
               label="iFL"
               mnemonic="M">
            <command
                  commandId="org.eclipse.sed.ifl.commands.startCommand"
                  id="org.eclipse.sed.ifl.menus.startCommand"
                  mnemonic="i"
                  icon="icons/ico16.png">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="org.eclipse.sed.ifl.toolbars.toolbar">
            <command
                  id="org.eclipse.sed.ifl.toolbars.startCommand"
                  commandId="org.eclipse.sed.ifl.commands.startCommand"
                  icon="icons/ico16.png"
                  tooltip="Start interactive Fault Localization session.">
            </command>
<!--            <command
                  commandId="org.eclipse.sed.ifl.test"
                  label="TEST"
                  style="push">
            </command>-->
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            id="org.eclipse.sed.ifl.preferences.IFLPreferencePage"
            name="iFL Preferences"
            class="org.eclipse.sed.ifl.ide.preferences.IFLPreferencePage">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.eclipse.sed.ifl.ide.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.eclipse.sed.ifl"
            name="Fault Localization">
      </category>
      <view
            allowMultiple="false"
            category="org.eclipse.sed.ifl.filter"
            class="org.eclipse.sed.ifl.ide.gui.FilterPart"
            icon="icons/ico16.png"
            id="org.eclipse.sed.ifl.views.IFLFilterView"
            inject="true"
            name="Interactive Fault Localization Filters"
            restorable="false">
      </view>
      <view
            allowMultiple="false"
            category="org.eclipse.sed.ifl.sorting"
            class="org.eclipse.sed.ifl.ide.gui.DualListPart"
            icon="icons/ico16.png"
            id="org.eclipse.sed.ifl.views.IFLDualListView"
            inject="true"
            name="Interactive Fault Localization Sorting"
            restorable="false">
      </view>
      <view
            allowMultiple="false"
            category="org.eclipse.sed.ifl"
            class="org.eclipse.sed.ifl.ide.gui.MainPart"
            icon="icons/ico16.png"
            id="org.eclipse.sed.ifl.views.IFLMainView"
            inject="true"
            name="Interactive Fault Localization"
            restorable="false">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="org.eclipse.sed.ifl.views.IFLMainView"
               relative="org.eclipse.ui.views.ProblemView"
               relationship="stack"
               ratio="0.5"
               visible="false">
         </view>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.eclipse.sed.ifl.ide.handlers.startHandler"
            commandId="org.eclipse.sed.ifl.commands.startCommand">
      </handler>
   </extension>
</plugin>
